// App.js
import React, { useState } from 'react';
function Child({ name }) {
return <h3>Hello, {name}!</h3>;
}
function App() {
const [count, setCount] = useState(0);
return (
<div>
<Child name="Anjali" />
<p>Count: {count}</p>
<button onClick={() => setCount(count + 1)}>Increase</button>
</div>
);
}
export default App;

OUTPUT
Hello, Anjali!
Count: 0
[Increase button â†’ updates count]
