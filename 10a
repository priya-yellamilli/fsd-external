// App.js
import React, { Component } from 'react';
// Function component
function Child({ name }) {
return <h3>Child says: Hello {name}!</h3>;
}
// Class component
class Parent extends Component {
render() {
return (
<div>
<h2>Parent Component</h2>
<Child name="Anjali" />
</div>
);
}
}
function App() {
return <Parent />;
}
export default App;
OUTPUT
Parent Component
Child says: Hello Anjali!
